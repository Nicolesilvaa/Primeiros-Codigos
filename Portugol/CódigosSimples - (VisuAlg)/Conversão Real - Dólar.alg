Algoritmo "Conversão Real - Dólar"

// Data : 27/07/2020

Var

D,Conversao ,R: Real

Inicio

D <- 0
R <- 0
Escreval(" Qual o valor da cotação do dólar ")
Leia(D)
Escreval(" Qual a quantidade de reais disponivel ?")
Leia(R)
Conversao <- R * D
Escreval("Sua quantia em reais é igual a :" , D , " dólares")


Fimalgoritmo
