algoritmo "LC01-Porcentagem"

// Data : 23/08/2020

var
I, G1,G2,G3 :Real
inicio
I <-780.000
<-0
G1 <- I * 46/100
G2 <- I - G1  * 32/100
G3 <- I - G1 + G2
Escreval("O primeiro ganhador recebera a quantia de :",G1:5:3 ,"$")
Escreval("O segundo ganhador recebera a quantia de :",G2:5:3 ,"$")
Escreval("O terceiro ganhador recebera a quantia de :",G3:5:3 ,"$")
fimalgoritmo
