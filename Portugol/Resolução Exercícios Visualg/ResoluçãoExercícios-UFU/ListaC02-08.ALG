Algoritmo "LC02-08"
// Data atual  : 6/2/2021

Var

   Nota1,Nota2:inteiro

Inicio


   Nota1 <- 0

   Nota2 <- 0

   Escreval("Digite 2 notas")

   Leia(Nota1,Nota2)

   Se(Number1 >= 0)e(Number1 <= 10)e(Number2 >= 0)e(Number2 <= 10)Entao
         Escreval("As notas são válidas")
   Senao
      Escreval("As notas não são válidas")
   Fimse

Fimalgoritmo