Algoritmo "LC02-12"

// Data atual  : 7/2/2021
Var

   Number:Inteiro
   logaritmo : real


Inicio

   Number<- 0
   Escreval("Digite um número")
   Leia(Number)
   Se(Number< 0) entao
      Escreval("Número inválido")
   Senao
      logaritmo <-log(Number)
   Fimse
Fimalgoritmo
