algoritmo "Troca de Valores de Variavéis"

// Data : 27/07/2020

var
  A,B: Inteiro
inicio
A <- 0
B <- 0
   Escreval("Determine um valor para A e B")
   Leia(A , B)
   Escreval("Os valores escolhidos são: ", " A = " ,A, " e  B = ", B )
    A <- B
    B <- A
   Escreval ("O valor de A é: ", A, " O valor de B é: ", B)

fimalgoritmo
