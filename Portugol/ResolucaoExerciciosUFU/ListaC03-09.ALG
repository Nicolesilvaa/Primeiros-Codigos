Algoritmo "LC03-09"

// Data atual  : 14/3/2021
Var
   i,n: inteiro


Inicio

   i <-0
   n <-0
   Escreval("Digite um valor")
   leia(n)
   Para i de 1 ate n passo 1 faca
      Se(i % 2 <> 0)entao
         Escreval(i)
      Fimse
   FimPara
   Escreval("Estes são os ", n ," primeiros números ímpares ")

Fimalgoritmo